<?xml version="1.0" encoding="utf-8"?>
<resources>

	<!-- action-bar dark
    <!-- color name: primary
		is used in Ti.DrawerLayout/android/platform/android/res/layout/drawer_main.xml
		(https://github.com/manumaticx/Ti.DrawerLayout)
		for action bar's background color
		do not change this name!
		-->
	<color name="primary">#fff</color>
	<color name="colorPrimaryDark">#082e3d</color>
	<color name="hintTextColor">#91082e3d</color>
	<color name="textColorPrimary">#ffffff</color>
	<color name="navigationBarColor">#082e3d</color>

	<!-- action-bar
		- actionBarButtonColor does not work, please use colored icon for this.
		- actionBarTitleFont and actionBarSubtitleFont do not work,
		  use https://github.com/ricardoalcocer/actionbarextras for custom fonts.
		-->
	<!-- <color name="actionBarButtonColor">#ffffff</color> -->
	<color name="actionBarBackButtonColor">#05f199</color>
	<color name="actionBarTitleColor">#05f199</color>
	<!-- <string name="actionBarTitleFont">Lato-BlackItalic</string> -->
    <dimen name="actionBarTitleFontSize">16dp</dimen>
	<color name="actionBarSubtitleColor">#ffffff</color>
	<!-- <string name="actionBarSubtitleFont">Lato-BlackItalic</string> -->
	<dimen name="actionBarSubtitleFontSize">14dp</dimen>
	<string name="actionBarBackgroundGradientAngle">0</string>
	<color name="actionBarBackgroundColor">#082e3d</color>
	<color name="actionBarBackgroundGradientStartColor">#082e3d</color>
    <color name="actionBarBackgroundGradientEndColor">#00425c</color>


</resources>
